[<Start>]
value: string | number | objects | array1  | 'true' | 'false' | 'null' 

string: '"' char* '"'
char: 'a' | 'b' | 'c' | 'd' | 'e' | 'f' | 'g' | 'h' | 'i' | 'j' | 'k' | 'l' | 'm' | 'n' | 'o' | 'p' | 'q' | 'r' | 's' | 't' | 'u' | 'v' | 'w' | 'x' | 'y' | 'z' 
  
digit: '1' | '2' | '3' | '4' | '5' | '6' | '7' | '8' | '9' | '0'
number: '-'? digit+

array1: '[' array_items ']' 

pair: string ':' value 
 
objects: '{' pair_items '}'

array_items: value | value ',' array_items
pair_items: pair | pair ',' pair_items
