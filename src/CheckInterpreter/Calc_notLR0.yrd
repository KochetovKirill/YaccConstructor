[<Start>]
calc : statement EOF { $1 }

statement : expr { $1 }
	      | ID EQ expr { $3 }

expr : multExpression PLUS multExpression { $1 + $3 }
     | multExpression MINUS multExpression { $1 - $3 }
     | multExpression { $1}
atom : ID { $1 }
	| INT { $1 }
	| LBRACE expr RBRACE { $2 }
	
multExpression : atom MULT atom { $1 * $3 }
               | atom DIV atom { $1 / $3 }
	           | atom {$1}
