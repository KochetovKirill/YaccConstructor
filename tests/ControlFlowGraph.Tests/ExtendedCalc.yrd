{
open QuickGraph.FST.GraphBasedFst
open QuickGraph.FSA.GraphBasedFsa
open Yard.Utils.StructClass
}

tokens {
    _ of FSA<char*Position<string>>
}

options {
    infEpsPath = epsilons
    pos = uint64
}


[<Start>]
start: assign+

assign: id ASSIGN expr SEMICOLON

expr: expr op expr
	 | NUMBER 
	 | id
         | LBRACE expr RBRACE

op: PLUS | MINUS | MULT | DIV
id: ID