yard_option<<item>>: 
      <yard_item> = item { Some yard_item }
    | empty { None };

yard_nlist<<item>>: 
      <yard_h> = item { [yard_h] }
    | <yard_h> = item <yard_t> = yard_nlist<<item>> { yard_h::yard_t };

yard_list<<item>>: 
      empty { [] }
    | <yard_h> = item <yard_t> = yard_list<<item>> { yard_h::yard_t };

yard_item_1<<a>>:  NUMBER;

t<<a>>:  yard_list<<yard_item_1<<a>>>>;

[<Start>]
s: t<<b>>;
