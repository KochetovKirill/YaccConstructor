[<Start>]
s:  yard_rule_perm3_1;

yard_rule_perm3_1: 
      v0 = a {v1,v2} = yard_rule_perm2_2 {v0, v1, v2}
    | v1 = b {v0,v2} = yard_rule_perm2_3 {v0, v1, v2}
    | v2 = c {v0,v1} = yard_rule_perm2_4 {v0, v1, v2};

yard_rule_perm2_2: 
      v0 = b v1 = c {v0,v1}
    | v1 = c v0 = b {v0,v1};

yard_rule_perm2_3: 
      v0 = a v1 = c {v0,v1}
    | v1 = c v0 = a {v0,v1};

yard_rule_perm2_4: 
      v0 = a v1 = b {v0,v1}
    | v1 = b v0 = a {v0,v1};

